---
---
{
	"version": "https://jsonfeed.org/version/1",
	"title": "Between a rock and a crazy place",
	"home_page_url": "https://blog.merovius.de/",
	"feed_url": "https://blog.merovius.de/feed.json",
	"author": {
		"name": "Axel Wagner"
	},
	"items": [{% for post in site.posts %}
		{
			"id": "{{ site.url }}{{ post.id }}",
			"url": "{{ site.url }}{{ post.url }}",
			"title": "{{ post.title }}",
			"content_html": "{{ post.content }}",
			{% if post.tldr %}"summary": "{{ post.tldr }}",{% endif %}
			"date_published": "{{ post.date | date_to_xmlschema }}"
		}{% unless forloop.last %},{% endunless %}{% endfor %}
	]
}
